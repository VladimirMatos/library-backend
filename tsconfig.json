{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "target": "es2017",
    "sourceMap": true,
    "outDir": "./dist",
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@/userEntity/*": ["src/users/entity/*"],
      "@/userService/*": ["src/users/services/*"],
      "@/userController/*": ["src/users/controllers/*"],
      "@/userDoc/*": ["src/users/doc/*"],
      "@/userDto/*": ["src/users/dto/*"],
      "@/userRepository/*": ["src/users/repository/*"],
      "@/userModule/*": ["src/users/*"],

      "@/rolesEntity/*": ["src/roles/entity/*"],
      "@/rolesService/*": ["src/roles/services/*"],
      "@/rolesController/*": ["src/roles/controllers/*"],
      "@/rolesDoc/*": ["src/roles/doc/*"],
      "@/rolesModule/*": ["src/roles/*"],

      "@/categoryEntity/*": ["src/category/entity/*"],
      "@/categoryService/*": ["src/category/services/*"],
      "@/categoryController/*": ["src/category/controllers/*"],
      "@/categoryDoc/*": ["src/category/doc/*"],
      "@/categoryModule/*": ["src/category/*"],

      "@/bookRentEntity/*": ["src/book-rent/entity/*"],
      "@/bookRentService/*": ["src/book-rent/services/*"],
      "@/bookRentController/*": ["src/book-rent/controllers/*"],
      "@/bookRentDoc/*": ["src/book-rent/doc/*"],
      "@/bookRentDto/*": ["src/book-rent/dto/*"],
      "@/bookRentRepository/*": ["src/book-rent/repository/*"],
      "@/bookRentModule/*": ["src/book-rent/*"],

      "@/bookPageEntity/*": ["src/book-page/entity/*"],
      "@/bookPageService/*": ["src/book-page/services/*"],
      "@/bookPageDoc/*": ["src/book-page/doc/*"],
      "@/bookPageModule/*": ["src/book-page/*"],
      "@/bookPageDto/*": ["src/book-page/dto/*"],

      "@/bookEntity/*": ["src/book/entity/*"],
      "@/bookService/*": ["src/book/services/*"],
      "@/bookController/*": ["src/book/controllers/*"],
      "@/bookDoc/*": ["src/book/doc/*"],
      "@/bookDto/*": ["src/book/dto/*"],
      "@/bookRepository/*": ["src/book/repository/*"],
      "@/bookModule/*": ["src/book/*"],

      "@/authEntity/*": ["src/auth/entity/*"],
      "@/authService/*": ["src/auth/services/*"],
      "@/authDto/*": ["src/auth/dto/*"],
      "@/authController/*": ["src/auth/controllers/*"],
      "@/authModule/*": ["src/auth/*"],
    },
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "forceConsistentCasingInFileNames": false,
    "noFallthroughCasesInSwitch": false
  }
}
